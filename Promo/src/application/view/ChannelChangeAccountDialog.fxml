<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.ChannelChangeAccountDialogController">
   <children>
      <Label layoutX="110.0" layoutY="34.0" text="Change account">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <GridPane layoutX="50.0" layoutY="105.0" prefWidth="300.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="145.0" minWidth="10.0" prefWidth="86.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="214.0" minWidth="10.0" prefWidth="214.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Name" />
            <Label text="Daily price" GridPane.rowIndex="1" />
            <Label text="Night price" GridPane.rowIndex="2" />
            <TextField fx:id="name" GridPane.columnIndex="1" />
            <TextField fx:id="dayPrice" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="nightPrice" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <Button layoutX="272.0" layoutY="262.0" mnemonicParsing="false" onAction="#handleOk" prefHeight="24.0" prefWidth="49.0" text="Ok" />
      <Button layoutX="337.0" layoutY="262.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
   </children>
</AnchorPane>
